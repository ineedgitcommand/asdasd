      "node_modules/figlet/fonts/Standard.flf"
    ]
  },
  "dependencies": {
    "@types/node": "^12.7.5",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "fs": "0.0.1-security",
    "javascript-obfuscator": "^2.3.1",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.0",
    "nodemon": "^1.19.2",
    "pkg": "^4.4.0",
    "pm2": "^4.5.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "gulp": "gulp",
    "dev": "nodemon --ignore \"web/*\" dist/build/app.js -c config.json",
    "prod": "pm2 start ecosystem.config.js",
    "compile": "pkg . --output dist/build/Oniric"
